class Solution {
    public List<String> buildArray(int[] target, int n) {
        String pu="Push",po="Pop";
        int i=0,cnt=1,size=target.length;
        List<String> list = new ArrayList<>();
        while(i<=size-1 && cnt<=n){
           int ele=cnt;
           if(ele==target[i]) {
               list.add(pu);
               i++;
           }
           else {
               list.add(pu);
               list.add(po);
           }
           cnt++;
        }
        return list;
    }
}
